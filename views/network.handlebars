<!-- ng-app="myApp" -->
<link rel="stylesheet" href="/css/note.css"></style>
<div ng-controller="myController" ng-init="NetworkInit()">
<ol class="breadcrumb">
	<li class="breadcrumb-item">
		<a href="#">Network</a>
	</li>
	<li class="breadcrumb-item active">{[{Network.network_info.N_Name}]}</li>
</ol>
	<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
			<h2 class="h2">#{[{Network.network_info.N_Number}]} - {[{Network.network_info.N_Name}]}   {[{Network.network_info.Category}]}</h2>
      <h2 style="background-color:red;color:white; width: 237px; height: 40px; text-align: center;">CONFIDENTIAL</h2>
      <img src="/img/LDBLogo.png" width="200px">
			{{#if isAuthenticated}}
			<div class="btn-toolbar mb-2 mb-md-0">
				<div class="btn-group mr-2">
					<!-- <button class="btn btn-sm btn-outline-secondary" data-toggle="modal" data-target="#SaveNetwork" >Save</button> -->
					<button class="btn btn-sm btn-outline-danger" data-toggle="modal" data-target="#DeleteNetwork">Delete</button>
				</div>
			</div>
			{{/if}}
		</div>
		<a href="/diagram/{[{Network.network_info.id}]}">Network Diagarm</a>
		<hr>
	<div class = "row">
		<div class = "col-lg-8">
	<div class="card mb-3">
		<div class="card-header">
			<i class="fa fa-table"></i> End Device
			{{#if isAuthenticated}}
			<button class="btn btn-primary btn-sm pull-right" type="button" data-toggle="modal" data-target="#NewEndDevice"><i class="fa fa-plus" aria-hidden="true"></i> New</button>
			{{/if}}
			</div>
		<div class="card-body">
			<div class="table-responsive">
				<table class="table table-striped table-hover table-sm"  width="100%" cellspacing="0" id="EndDtable">
					<thead>
						<tr>
							<th>IP</th>
							<th>Device Name</th>
							<th>VLAN</th>
							<th>Port</th>
							<th>Active</th>
							<th>Description</th>
							<th>Type</th>
							<th>Make</th>
							<th>Model</th>
							<th>Connected Device</th>
							{{#if isAuthenticated}}
							<th>Action</th>
							{{/if}}
						</tr>
					</thead>
					<tbody>
						<tr ng-repeat="End_Device in Network.End_Devices">
								<td>{[{End_Device.ip}]}</td>
								<td>{[{End_Device.name}]}</td>
								<td>{[{End_Device.VLAN_ID}]}</td>
								<td>{[{End_Device.Port}]}</td>
								<td>{[{End_Device.Active}]}</td>
								<td>{[{End_Device.Description}]}</td>
								<td>{[{End_Device.Type}]}</td>
								<td>{[{End_Device.Make}]}</td>
								<td>{[{End_Device.Model}]}</td>
								<td>{[{End_Device.NetDevice}]}</td>
								{{#if isAuthenticated}}
								<td>
									<button class="btn btn-default btn-sm"  type="button" ng-click="selectEndDevice(End_Device)" data-toggle="modal" data-target="#EditEndDevice" ><i class="fa fa-pencil" aria-hidden="true"></i></button>
									<button class="btn btn-danger btn-sm" type="button" ng-click="selectEndDevice(End_Device)" data-toggle="modal" data-target="#myModalDelete" ><i class="fa fa-trash" aria-hidden="true"></i></button>
							</td>
							{{/if}}
						</tr>
					</tbody>
				</table>
			</div>
		</div>
		<div class="card-footer small text-muted">Last Updated at {[{Network.Update_history[0].Update_date | date: 'yyyy-MM-dd'}]}</div>
	</div>
</div>
<div class = "col-lg-4">
		<div class=" card mb-3">
		<div class="card-header">
			<i class="fa fa-table"></i> Network Information
			{{#if isAuthenticated}}
			<button class="btn btn-primary btn-sm pull-right" type="button" data-toggle="modal" data-target="#editNetwork" ng-click = "selectNetwork()"><i class="fa fa-pencil" aria-hidden="true"></i> Edit</button>
			{{/if}}
			</div>
			<div class="card-body">
				<table class="table">
				    <tbody>
							<tr>
							<th>Address</th>
								<td>{[{Network.network_info.address}]}</td>
						</tr>
						<tr>
							<th>Postal Code</th>
								<td>{[{Network.network_info.Postal_Code}]}</td>
						</tr>
						<tr>
							<th>Phone</th>
								<td>{[{Network.network_info.Phone}]}</td>
						</tr>
						<tr>
							<th>Fax</th>
								<td>{[{Network.network_info.Fax}]}</td>
						</tr>
						<tr>
							<th>Circuit_ID</th>
								<td>{[{Network.network_info.Circuit_ID}]}</td>
						</tr>
						</tbody>
					</table>
	</div>
	<div class="card-footer small text-muted">Last Updated at {[{Network.Update_history[0].Update_date | date: 'yyyy-MM-dd'}]}</div>
	</div>
    <div class="card mb-3">
        <div class="card-header" id="accordion">
            <i class="fa fa-history"></i> Notes
            <div class="btn-group pull-right">
                <a type="button" class="btn btn-primary btn-sm pull-right" data-toggle="collapse" data-parent="#accordion" data-target="#collapseOne">
                    <i class="fa fa-caret-down"></i>
                </a>
            </div>
        </div>
    <div class="card-collapse collapse" id="collapseOne">
                  <div class="card-body chatcard-body">
                      <ul class="chat" ng-repeat="note in Network.Update_history ">
                          <li class="left clearfix"><span class="chat-img pull-left" >
                              <img ng-if="note.admin_id == '1'"src="/img/A.png" alt="User Avatar" class="img-circle" />
															<img ng-if="note.admin_id == '2'"src="/img/SA.png" alt="User Avatar" class="img-circle" />
                          </span>
                              <div class="chat-body clearfix">
                                  <div class="header">
                                      <strong class="primary-font">{[{note.email}]}</strong> <small class="pull-right text-muted">
                                          <span class="glyphicon glyphicon-time"></span>{[{note.Update_date | date: 'yyyy-MM-dd'}]}</small>
                                  </div>
																	<div class="row">
																		<div class="col-lg-10">
	                                  	<p>{[{note.Description}]}</p>
																		</div>
																		{{#if isAuthenticated}}
																		<div class="col-lg-2">
																			<div class="row">
																				<button class="btn btn-default btn-sm"  type="button" ng-click="selectNote(note)" data-toggle="modal" data-target="#UpdateNote" ><i class="fa fa-pencil" aria-hidden="true"></i></button>
																				 <button class="btn btn-danger btn-sm" type="button" ng-click="selectNote(note)" data-toggle="modal" data-target="#deleteNote" ><i class="fa fa-trash" aria-hidden="true"></i></button>
																			</div>
																		</div>
																		{{/if}}
																	</div>
                              </div>
                          </li>
                      </ul>
                  </div>
									{{#if isAuthenticated}}
                  <div class="card-footer">
                      <div class="input-group">
                        <!-- <input  id="btn-input" type="text" class="form-control input-sm" placeholder="Type your message here..." /> -->
                        <textarea class="form-control" aria-label="With textarea" ng-model="newUpdate.Description"></textarea>
                          <span class="input-group-btn">
                              <button class="btn btn-primary btn-sm" id="btn-chat" ng-click="post_update()">
                                  Send</button>
                          </span>
                      </div>
											{{/if}}
                  </div>
              </div>
          </div>
        </div>
    </div>
	<div class="card mb-3">
		<div class="card-header">
			<i class="fa fa-table"></i> WAN
			{{#if isAuthenticated}}
			<button class="btn btn-primary btn-sm pull-right" type="button" data-toggle="modal" data-target="#NewWAN"><i class="fa fa-plus" aria-hidden="true"></i> New</button>
			{{/if}}
		</div>
		<div class="card-body">
			<div class="table-responsive">
				<table class="table table-striped table-hover table-sm" id="WANTable" width="100%" cellspacing="0">
					<thead>
						<tr>
							<th>Provider</th>
							<th>WAN Type</th>
							<th>Upload</th>
							<th>DownLoad</th>
							<th>UploadShaping</th>
							<th>WAN Network</th>
							<th>WAN IP</th>
							<th>ADSL/OOB</th>
							<th>LOC ID</th>
							<th>ASSET TAG</th>
							<th>CSID</th>
							{{#if isAuthenticated}}
							<th>Action</th>
							{{/if}}
						</tr>
					</thead>
					<tbody>
						<tr ng-repeat="WAN in Network.WANs">
							<td>{[{WAN.Provider}]}</td>
							<td>{[{WAN.WAN_Type}]}</td>
							<td>{[{WAN.Upload}]}</td>
							<td>{[{WAN.DownLoad}]}</td>
							<td>{[{WAN.UploadShaping}]}</td>
							<td>{[{WAN.WAN_Network}]}</td>
							<td>{[{WAN.WAN_IP}]}</td>
							<td>{[{WAN.ADSL_OOB}]}</td>
							<td>{[{WAN.LOC_ID}]}</td>
							<td>{[{WAN.ASSET_TAG}]}</td>
							<td>{[{WAN.CSID}]}</td>
							{{#if isAuthenticated}}
							<td>
								 <button class="btn btn-default btn-sm"  type="button" ng-click="selectWAN(WAN)" data-toggle="modal" data-target="#EditWAN" ><i class="fa fa-pencil" aria-hidden="true"></i></button>
									<button class="btn btn-danger btn-sm" type="button" ng-click="selectWAN(WAN)" data-toggle="modal" data-target="#DeleteWAN" ><i class="fa fa-trash" aria-hidden="true"></i></button>
								 </td>
						</tr>
						{{/if}}
					</tbody>
				</table>
			</div>
		</div>
		<div class="card-footer small text-muted">Last Updated at {[{Network.Update_history[0].Update_date | date: 'yyyy-MM-dd'}]}</div>
	</div>
	<div class="row">
		<div class = "col-lg-6" >
			<div class="card mb-3">
				<div class="card-header">
					<i class="fa fa-table"></i> Router/Switch
					{{#if isAuthenticated}}
					<button class="btn btn-primary btn-sm pull-right" type="button" data-toggle="modal" data-target="#NewNetDevice"><i class="fa fa-plus" aria-hidden="true"></i> New</button>
					{{/if}}
				</div>
				<div class="card-body">
					<div class="table-responsive">
						<table class="table table-striped table-hover table-sm" id="NDeviceTable" width="100%" cellspacing="0">
							<thead>
								<tr>
									<th>Type</th>
									<th>Name</th>
									<th>Loopback</th>
									<th>Make</th>
									<th>Model</th>
									<th>Serial Number</th>
									<th>Connect Device</th>
									{{#if isAuthenticated}}
									<th>Action</th>
									{{/if}}
								</tr>
							</thead>
							<tbody>
								<tr ng-repeat="Net_Device in Network.Net_devices | filter:{Status:'!-1'}">
									<td>{[{Net_Device.type}]}</td>
									<td>{[{Net_Device.name}]}</td>
									<td>{[{Net_Device.Loopback}]}</td>
									<td>{[{Net_Device.Make}]}</td>
									<td>{[{Net_Device.Model}]}</td>
									<td>{[{Net_Device.Serial_Number}]}</td>
									<td>{[{Net_Device.Connect_Device}]}</td>
									{{#if isAuthenticated}}
									<td>
										  <button class="btn btn-default btn-sm"  type="button" ng-click="selectNetDevice(Net_Device)" data-toggle="modal" data-target="#EditNetDevice" ><i class="fa fa-pencil" aria-hidden="true"></i></button>
											<button class="btn btn-danger btn-sm" type="button" ng-click="selectNetDevice(Net_Device)" data-toggle="modal" data-target="#DeleteNetDevice" ><i class="fa fa-trash" aria-hidden="true"></i></button>
									</td>
									{{/if}}
								</tr>
							</tbody>
						</table>
					</div>
				</div>
				<div class="card-footer small text-muted">Last Updated at {[{Network.Update_history[0].Update_date | date: 'yyyy-MM-dd'}]}</div>
			</div>
		</div>
		<div class = "col-lg-6" >
			<div class="card mb-3">
				<div class="card-header">
					<i class="fa fa-table"></i> VLANS
					{{#if isAuthenticated}}
					<button class="btn btn-primary btn-sm pull-right" type="button" data-toggle="modal" data-target="#NewVLAN"><i class="fa fa-plus" aria-hidden="true"></i> New</button>
					{{/if}}
				</div>
				<div class="card-body">
					<div class="table-responsive">
						<table class="table table-striped  table-hover table-sm" id="VLANTable" width="100%" cellspacing="0">
							<thead>
								<tr>
									<th>VlAN</th>
									<th>description</th>
									<th>Gateway</th>
									<th>Subnet</th>
									{{#if isAuthenticated}}
									<th>Action</th>
									{{/if}}
								</tr>
							</thead>
							<tbody>
								<tr ng-repeat="vlan in Network.VlanNetwork  | filter:{Status:'!-1'}">
									<td>{[{vlans[vlan.vlan_id-1].vlan_number}]}</td>
									<td>{[{vlans[vlan.vlan_id-1].Description}]}</td>
									<td>{[{vlan.Gateway}]}</td>
									<td>{[{vlan.Submusk}]}</td>
									{{#if isAuthenticated}}
									<td>
										<button class="btn btn-default btn-sm"  type="button" ng-click="selectedVLAN(vlan)" data-toggle="modal" data-target="#editVlan" ><i class="fa fa-pencil" aria-hidden="true"></i></button>
										<button class="btn btn-danger btn-sm" type="button" ng-click="selectedVLAN(vlan)" data-toggle="modal" data-target="#deleteVlan" ><i class="fa fa-trash" aria-hidden="true"></i></button>
									 </td>
									 {{/if}}
								</tr>
							</tbody>
						</table>
					</div>
				</div>
				<div class="card-footer small text-muted">Last Updated at {[{Network.Update_history[0].Update_date | date: 'yyyy-MM-dd'}]}</div>
			</div>
		</div>
	</div>

 <!-- Trigger the END Device modal with a button -->

<div id="NewEndDevice" class="modal fade" role="dialog" >
  <div class="modal-dialog  modal-lg"  >
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
				<h4 class="modal-title">New End Device</h4>
        <button type="button" class="close" data-dismiss="modal">&times;</button>
      </div>
      <div class="modal-body">
	      <form>
					<div class="container col-md-12">
						<div class="row" >
							<div class="col-md-4">
					    	<label for="inputEmail3"><strong>IP</strong></label>
					      <input type="text" class="form-control" id="inputEmail3" placeholder="IP Address" ng-model="NewEndevice.ip">
							</div>
							<div class="col-md-4">
					    	<label for="DeviceName"><strong>Device Name</strong></label>
					      <input type="text" class="form-control" id="DeviceName" placeholder="Device Name" ng-model="NewEndevice.name">
							</div>
							<div class="col-md-4">
							<label for="inputEmail3" ><strong>VLAN</strong></label>
							<input type="number" class="form-control" id="inputEmail3" placeholder="VLAN" ng-model="NewEndevice.VLAN_ID">
						</div>
						</div>
					</div>
					<div class="container col-md-12">
						<div class="row">
							<div class="col-md-2">
									<label for="inputEmail3" class="control-label"><strong>Port</strong></label>
									<input type="text" class="form-control" id="inputEmail3" placeholder="Port" ng-model="NewEndevice.Port">
								</div>
								<div class="col-md-2">
									<label for="inputEmail3" class="control-label"><strong>Active</strong></label>
									<input type="text" class="form-control" id="inputEmail3" placeholder="Active" ng-model="NewEndevice.Active">
								</div>
								<div class="col-md-8">
									<label for="inputEmail3" class="control-label"><strong>Description</strong></label>
									<input type="text" class="form-control" id="inputEmail3" placeholder="Description" ng-model="NewEndevice.Description">
								</div>
							</div>
						</div>
						<div class="container col-md-12">
							<div class="row">
								<div class="col-md-3">
									<label for="inputEmail3" class="control-label"><strong>Type</strong></label>
									<input type="text" class="form-control" id="inputEmail3" placeholder="Type" ng-model="NewEndevice.Type">
								</div>
									<div class="col-md-3">
										<label for="inputEmail3" class="ccontrol-label"><strong>Make</strong></label>
										<input type="text" class="form-control" id="inputEmail3" placeholder="Make" ng-model="NewEndevice.Make">
									</div>
									<div class="col-md-3">
										<label for="inputEmail3" class="control-label"><strong>Model</strong></label>
										<input type="text" class="form-control" id="inputEmail3" placeholder="Model" ng-model="NewEndevice.Model">
									</div>
										<div class="col-md-3">
											<label for="inputEmail3" class=" control-label"> <strong>Connected Device</strong></label>
											<input type="text" class="form-control" id="inputEmail3" placeholder="Connected Device" ng-model="NewEndevice.NetDevice">
										</div>
							</div>
						</div>
			      </div>
			      <div class="modal-footer">
							<button type="button" class="btn btn-primary " data-dismiss="modal" ng-click="addEndDevice();">Save</button>
			        <button type="button" class="btn btn-default" data-dismiss="modal" ng-click="NewEndevice={}">Close</button>
			      </div>
						</form>
					</div>
			    </div>
			  </div>

