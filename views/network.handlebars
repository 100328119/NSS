<div ng-controller="myController" ng-init="NetworkInit()">
<ol class="breadcrumb">
	<li class="breadcrumb-item">
		<a href="#">Network</a>
	</li>
	<li class="breadcrumb-item active">{[{Network.network_info.N_Name}]}</li>
</ol>
	<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
			<h2 class="h2">#{[{Network.network_info.N_Number}]} - {[{Network.network_info.N_Name}]}   {[{Network.network_info.Category}]}</h2>
			{{#if isAuthenticated}}
			<div class="btn-toolbar mb-2 mb-md-0">
				<div class="btn-group mr-2">
					<button class="btn btn-sm btn-outline-secondary" data-toggle="modal" data-target="#SaveNetwork" >Save</button>
					<button class="btn btn-sm btn-outline-secondary" data-toggle="modal" data-target="#DeleteNetwork">Delete</button>
				</div>
			</div>
			{{/if}}
		</div>
		<a href="/diagram/{[{Network.network_info.id}]}">Network Diagarm</a>
		<hr>
	<div class = "row">
		<div class = "col-lg-8">
	<div class="card mb-3">
		<div class="card-header">
			<i class="fa fa-table"></i> End Device
			{{#if isAuthenticated}}
			<button class="btn btn-primary btn-sm pull-right" type="button" data-toggle="modal" data-target="#NewEndDevice"><i class="fa fa-plus" aria-hidden="true"></i> New</button>
			{{/if}}
			</div>
		<div class="card-body">
			<div class="table-responsive">
				<table class="table table-striped table-hover table-sm"  width="100%" cellspacing="0" id="EndDtable">
					<thead>
						<tr>
							<th>IP</th>
							<th>Device Name</th>
							<th>VLAN</th>
							<th>Port</th>
							<th>Active</th>
							<th>Description</th>
							<th>Type</th>
							<th>Make</th>
							<th>Model</th>
							<th>Connected Device</th>
							{{#if isAuthenticated}}
							<th>Action</th>
							{{/if}}
						</tr>
					</thead>
					<tbody>
						<tr ng-repeat="End_Device in Network.End_Devices | filter:{Status:'!-1'}">
								<td>{[{End_Device.ip}]}</td>
								<td>{[{End_Device.name}]}</td>
								<td>{[{End_Device.VLAN_ID}]}</td>
								<td>{[{End_Device.Port}]}</td>
								<td>{[{End_Device.Active}]}</td>
								<td>{[{End_Device.Description}]}</td>
								<td>{[{End_Device.Type}]}</td>
								<td>{[{End_Device.Make}]}</td>
								<td>{[{End_Device.Model}]}</td>
								<td>{[{End_Device.NetDevice}]}</td>
								{{#if isAuthenticated}}
								<td>
									<button class="btn btn-default btn-sm"  type="button" ng-click="selectItem(End_Device)" data-toggle="modal" data-target="#EditEndDevice" ><i class="fa fa-pencil" aria-hidden="true"></i></button>
									<button class="btn btn-danger btn-sm" type="button" ng-click="selectItem(End_Device)" data-toggle="modal" data-target="#myModalDelete" ><i class="fa fa-trash" aria-hidden="true"></i></button>
							</td>
							{{/if}}
						</tr>
					</tbody>
				</table>
			</div>
		</div>
		<div class="card-footer small text-muted">Updated at {[{Network.Update_history[0].Update_date}]}</div>
	</div>
</div>
