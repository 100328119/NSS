	<div class="modal fade" id="UpdateNote" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">New message</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form>
          <div class="form-group">
            <label for="recipient-name" class="col-form-label">Date:</label>
            <input type="text" class="form-control" id="recipient-name" ng-model="currentdate" disabled>
          </div>
          <div class="form-group">
            <label for="message-text" class="col-form-label">Message:</label>
            <textarea class="form-control" id="message-text" ng-model="selectedItem.Description"></textarea>
          </div>
        </form>
      </div>
      <div class="modal-footer">
				<button type="button" class="btn btn-primary" ng-click="UpdateNote()" data-dismiss="modal">Update Notes</button>
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
	<div id="deleteNote" class="modal fade" role="dialog" >
		<div class="modal-dialog modal-dialog-center">
			<!-- Modal content-->
			<div class="modal-content">
				<div class="modal-header">
					<h4 class="modal-title">Delete Notes</h4>
					<button type="button" class="close" data-dismiss="modal">&times;</button>
				</div>
				<div class="modal-body">
					<p>{[{selectedItem.Description}]}</p>
				</div>
				<div class="modal-footer">
				<button type="button" class="btn btn-primary " data-dismiss="modal" ng-click="deleteNote()">Confirm</button>
					<button type="button" class="btn btn-default pull-right" data-dismiss="modal">Cancel</button>
				</div>
			</div>
		</div>
	</div>

</div>
